generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pet {
  id        String @id @default(cuid())
  name      String
  ownerName String
  photo     String
  age       Int
  notes     String
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now()) 
}
